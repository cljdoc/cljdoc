{
  "name": "cljdoc",
  "scripts": {
    "format": "prettier --write 'js/**/*.{js,jsx,ts,tsx}'",
    "lint": "prettier --check 'js/**/*.{js,jsx,ts,tsx}'",
    "dev": "parcel watch resources/public/main.html --dist-dir resources-compiled/public/out --public-url /",
    "build": "parcel build resources/public/main.html --dist-dir resources-compiled/public/out --public-url /",
    "prepare": "husky install",
    "typecheck": "npx tsc --watch"
  },
  "dependencies": {
    "date-fns": "^2.24.0",
    "fuzzysort": "^1.1.4",
    "preact": "^10.5.14"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "husky": "^6.0.0",
    "parcel": "^2.0.0-rc.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.3.5"
  },
  "staticFiles": {
    "staticPath": "resources/public/static"
  }
}
